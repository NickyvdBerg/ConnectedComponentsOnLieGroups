     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"method", "==", "\"\<LIF\>\""}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"\[Lambda]1", "=", 
         RowBox[{"Hess", "\[LeftDoubleBracket]", 
          RowBox[{"All", ",", "All", ",", "All", ",", "1", ",", "1"}], 
          "\[RightDoubleBracket]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"c", "=", 
         RowBox[{"Hess", "\[LeftDoubleBracket]", 
          RowBox[{"All", ",", "All", ",", "All", ",", "2", ",", "2"}], 
